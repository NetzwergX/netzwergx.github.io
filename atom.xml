---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<author>
		<name>Sebastian Teumert</name>
	</author>
	<title>Sebastian Teumert</title>
	<link href="http://teumert.net/atom.xml" rel="self" />
	<link href="http://teumert.net/" />
	<id>http://teumert.net/</id>
  	<updated>{% assign post = site.posts | first %}{{ post.date | date : '%Y-%m-%dT%H:%M:%S'}}+02:00</updated>
 
	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://teumert.net/{{ post.url }}" />
		<id>http://teumert.net/{{ post.url }}</id>
		<updated>{{ post.date | date : '%Y-%m-%dT%H:%M:%S'}}+02:00</updated>
		<summary><![CDATA[ {{ post.excerpt }}]]></summary>
		<content type="html">
			<![CDATA[{{ post.content }}]]>
		</content>
	</entry>
	{% endfor %}
</feed>
